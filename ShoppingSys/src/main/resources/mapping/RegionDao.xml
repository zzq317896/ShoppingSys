<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzq.dao.RegionDao">
	<select id="find" resultType="Region">
		select * from region
	</select>
	<select id="findById" parameterType="int" resultType="Region">
		select * from region where id=#{id}
	</select>
</mapper> 