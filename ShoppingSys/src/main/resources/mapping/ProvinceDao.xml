<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 映射文件与持久化接口通过nameSpace属性进行关联， -->
<mapper namespace="com.zzq.dao.ProvinceDao">
	
	<select id="findById" parameterType="int" resultType="Province">
		select * from Province where id= #{id}
	</select>
	<select id="find"  resultType="Province">
		select * from Province
	</select>
</mapper> 