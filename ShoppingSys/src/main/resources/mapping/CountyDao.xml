<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzq.dao.CountyDao">
	<select id="find" resultType="County">
		select * from county
	</select>
	<select id="findById" parameterType="int" resultType="County">
		select * from county where id=#{id}
	</select>
</mapper> 